PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX lgd: <http://linkedgeodata.org/> 
PREFIX lgdo: <http://linkedgeodata.org/ontology/> 
SELECT * FROM <http://linkedgeodata.org> 
WHERE { 
 ?country a lgdo:Country . 
 ?country <http://linkedgeodata.org/property/capital_city> ?city . 
 ?country <http://linkedgeodata.org/property/wikipedia> ?name . 
}